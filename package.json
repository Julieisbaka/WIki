{
  "name": "markdown-diagram-system",
  "version": "0.0.1",
  "description": "A dynamic diagram system that generates diagrams based on interlinking Markdown files within a wiki.",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "test": "jest",
    "build": "webpack --mode production",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "validate-links": "node rules/pre-commit.js",
    "validate-wiki": "node rules/validate-wiki.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "express": "^4.21.2",
    "markdown-it": "^14.1.0",
    "socket.io": "^4.8.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.3.0",
    "d3": "^7.9.0",
    "reactflow": "11.0.5",
    "unified": "^11.0.5",
    "remark-parse": "^11.0.0",
    "remark-frontmatter": "^5.0.0",
    "localforage": "^1.10.0",
    "styled-components": "^6.1.15",
    "react-colorful": "^5.6.1",
    "framer-motion": "^12.5.0",
    "react-zoom-pan-pinch": "^3.7.0",
    "@use-gesture/react": "^10.3.1",
    "axios": "^0.21.1",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "gh-pages": "^6.3.0",
    "husky": "^8.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate-links"
    }
  },
  "keywords": [
    "markdown",
    "diagram",
    "wiki",
    "nodejs"
  ],
  "author": "Your Name",
  "license": "MIT",
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Julieisbaka/WIki.git"
  },
  "bugs": {
    "url": "https://github.com/Julieisbaka/WIki/issues"
  },
  "homepage": "https://github.com/Julieisbaka/WIki#readme"
}
